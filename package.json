{
  "name": "pdf",
  "displayName": "vscode-pdf",
  "description": "Display pdf file in VSCode.",
  "version": "0.3.0",
  "publisher": "tomoki1207",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Languages"
  ],
  "keywords": [
    "pdf",
    "viewer"
  ],
  "icon": "vscode-pdf.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomoki1207/vscode-pdfviewer.git"
  },
  "bugs": {
    "url": "https://github.com/tomoki1207/vscode-pdfviewer/issues"
  },
  "activationEvents": [
    "onLanguage:pdf",
    "onCommand:extension.pdf-preview"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "languages": [
      {
        "id": "pdf",
        "aliases": [
          "Plain Text (PDF)",
          "pdf"
        ],
        "extensions": [
          ".pdf"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pdf",
        "scopeName": "text.plain, text.plain.pdf",
        "path": "./syntaxes/pdf.tmLanguage"
      }
    ],
    "commands": [
      {
        "command": "extension.pdf-preview",
        "title": "View PDF"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceLangId == pdf",
          "command": "extension.pdf-preview",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "vscode": "^1.0.0"
  }
}
