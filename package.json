{
  "name": "pdf",
  "displayName": "vscode-pdf",
  "description": "Display pdf file in VSCode.",
  "version": "0.6.0",
  "publisher": "tomoki1207",
  "engines": {
    "vscode": "^1.44.0"
  },
  "enableProposedApi": true,
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "pdf",
    "viewer"
  ],
  "icon": "vscode-pdf.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomoki1207/vscode-pdfviewer.git"
  },
  "bugs": {
    "url": "https://github.com/tomoki1207/vscode-pdfviewer/issues"
  },
  "activationEvents": [
    "onCustomEditor:pdf.preview",
    "onCommand:extension.pdf-preview"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "customEditors": [
      {
        "viewType": "pdf.preview",
        "displayName": "Pdf Preview",
        "selector": [
          {
            "filenamePattern": "*.pdf"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.pdf-preview",
        "title": "View PDF"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "dependencies": {
    "@types/vscode": "^1.44.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.21",
    "mocha": "^6.2.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3",
    "typings": "^1.4.0",
    "vsce": "^1.71.0",
    "vscode-dts": "^0.3.1",
    "vscode-test": "^1.3.0"
  }
}
